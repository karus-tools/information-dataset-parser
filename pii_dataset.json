        "regex_list": [
            "^\\s*\\d{1,4}\\s+((?:(?:[A-Z][a-z]*[\\s.'-]?)+)(?:(?:[A-Z][a-z]*[\\s.'-]?)+)?)\\s*,?\\s+((?:(?:[A-Z][a-z]*[\\s.'-]?)+)(?:(?:[A-Z][a-z]*[\\s.'-]?)+)?)\\s*,?\\s*((?:(?:[A-Z][a-z]*[\\s.'-]?)+)(?:(?:[A-Z][a-z]*[\\s.'-]?)+)?|(?:[A-Z]{2}\\d{1,2}[A-Z]?)\\s*\\d[A-Z]{2})\\s*$ \n",
            "^\\s*\\d{1,6}\\s+([a-zA-Z]+\\s+)+[a-zA-Z]{2,}\\s*,?\\s*[a-zA-Z]{2,}\\s+\\d{5}(?:-\\d{4})?\\s*$\n"
